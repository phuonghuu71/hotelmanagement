<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.StaffController">
   <children>
      <TableView fx:id="tableStaff" layoutX="14.0" layoutY="14.0" prefHeight="350.0" prefWidth="600.0">
         <columns>
            <TableColumn fx:id="colStaffID" prefWidth="75.0" text="ID" />
            <TableColumn fx:id="colUsername" prefWidth="150.0" text="Tên Đăng Nhập" />
            <TableColumn fx:id="colStaffName" prefWidth="150.0" text="Tên Nhân Viên" />
            <TableColumn fx:id="colType" minWidth="0.0" prefWidth="100.0" text="Chức Vụ" />
            <TableColumn fx:id="colSalary" prefWidth="100.0" text="Lương" />
         </columns>
      </TableView>
   </children>
</AnchorPane>
